{
  "name": "potsides",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://paul.potsides.com",
  "dependencies": {
    "history": "^4.7.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.2.0",
    "react-load-script": "0.0.6",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.0",
    "react-scripts": "5.0.1",
    "react-test-renderer": "^16.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "deploy": "",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "coverage": "npm test -- --coverage ",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "infrastructure": "terraform init ./infrastructure && terraform plan -out terraform.plan ./infrastructure && terraform apply terraform.plan"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/build/**",
      "!**/coverage/**",
      "!**/src/index.js",
      "!**/src/registerServiceWorker.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
